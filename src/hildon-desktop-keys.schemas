<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/launcher_navigator_accel</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/launcher_navigator_accel</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Use 15 keys to select icons in launcher and navigator</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/home_contacts_phone</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/home_contacts_phone</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Launch contacts or phone application on a key press in home</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/ctrl_backaspace</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/ctrl_backspace</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	      <short>Enable ctrl+backspace shortcut</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/dbus_shift_ctrl</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/dbus_shift_ctrl</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Capture all ctrl+shift+letter and report them on D-Bus</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/dbus_launcher_navigator</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/dbus_launcher_navigator</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Report key presses when in launcher or navigator on D-Bus</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/preset_shift_ctrl</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/preset_shift_ctrl</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Enable default ctrl-shift-[xnp] actions</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/ctrl_backspace_in_tasknav</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/ctrl_backspace_in_tasknav</applyto>
      <owner>hildon-desktop</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Action of ctrl+backspace when pressed while in navigator</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/dbus_ctrl_shortcuts</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/dbus_ctrl_shortcuts</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Capture some ctrl+key combinations (conflicts with other uses).</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/dbus_shortcuts_use_fn</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/dbus_shortcuts_use_fn</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Use ctrl+fn instead of ctrl+shift for keyboard shortcuts.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/osso/hildon-desktop/key-actions/disable_edit</key>
      <applyto>/apps/osso/hildon-desktop/key-actions/disable_edit</applyto>
      <owner>hildon-desktop</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Lock widgets on desktop.</short>
      </locale>
    </schema>
 
  </schemalist>
</gconfschemafile>
